import React, { useState } from 'react'

const App = () => { 
  
  const [num, setNum] = useState([10, 20, 30]);
  
  const btnClicked = () => {
  const newNum = [...num];
  newNum.push(40);
  setNum(newNum);
}

return(
  <div>
    <h1>{num} {num.age}</h1>
    <button onClick={btnClicked}>Change Number</button>
  </div>
)
}
export default App
